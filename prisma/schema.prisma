datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:mysecretpassword@localhost:5432/blog"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String   
  password  String
  typeUser  Int 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}